def count(value, lst):
    ans = 0  # Initialize the counter to 0
    element = 0  # Initialize the index variable to 0
    while element < len(lst):
        if lst[element] == value:
            ans += 1  # Increment the counter if the item matches the value
        element += 1  # Move to the next index
    return ans  # Return the final count


print(count(5,[1,2,3,4,5]))
print(count("ab", ["ab", "cd", "ab", "bc"]))
print(count("z", [1, 2, "a"]))
